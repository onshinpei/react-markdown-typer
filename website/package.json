{
  "name": "ds-markdown-website",
  "version": "1.0.0",
  "description": "ds-markdown 库的在线文档网站",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "dev:local": "SOURCE=local vite --mode development",
    "dev:npm": "SOURCE=npm vite --mode development",
    "start": "vite --mode development",
    "clean": "rimraf dist",
    "build": "npm run clean && vite build --mode production",
    "build:dev": "npm run clean && vite build --mode development",
    "preview": "vite preview",
    "serve": "vite preview",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "ds-markdown",
    "documentation",
    "markdown",
    "react",
    "typescript"
  ],
  "author": "ds-markdown team",
  "license": "MIT",
  "dependencies": {
    "ds-markdown": "^0.1.3-beta.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-syntax-highlighter": "^15.6.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  }
}
